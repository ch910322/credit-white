  GetWindowRecord(wn,RepSpec);
  mwn = MotherWindow(wn);
  GetWindowRecord(mwn,CredManr);
  CloseWindow(wn);
  if (RepSpec.ArtMode==0) then begin
    CreateCredManInvoiceDo(CredManr,RepSpec.f1,mwn);
  end else begin
    CredManMakePaymentRemote(CredManr,VIr,RepSpec.f1,sum,RepSpec.flags[10]==0);
    PutWindowRecord(mwn,CredManr);
    WindowDoOK(mwn,0);
    CM_CreatePaymentOrder(CredManr,VIr,sum);
    if (VIr.SerNr>0) then begin
      twn = OpenWindow("VIDClass",0,0,"","",VIr);
    end;
  end;

  return;
end;

