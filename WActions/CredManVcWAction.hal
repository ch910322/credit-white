  record RcVc RepSpec;

  if (changedf) then begin  
    GetWindowRecord(wn,RepSpec);
    if (RepSpec.vals3<(RepSpec.vals0 + RepSpec.vals1)) then begin
      RepSpec.vals3 = StringToVal(WindEFStr(wn),M4Val);
